    </form>
    <script>
        $('#myModal').on('shown.bs.modal', function () {
            $('#myInput').trigger('focus')
        })
    </script>

    <!-- Vendor -->
    <script src="assets/frontpage/vendor/jquery.appear/jquery.appear.min.js"></script>
    <script src="assets/frontpage/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="assets/frontpage/vendor/common/common.min.js"></script>
    <script src="assets/frontpage/vendor/owl.carousel/owl.carousel.min.js"></script>

    <!-- Theme Base, Components and Settings -->
    <script src="assets/frontpage/js/theme.js"></script>

    <!-- Theme Initialization Files -->
    <script src="assets/frontpage/js/theme.init.js"></script>

