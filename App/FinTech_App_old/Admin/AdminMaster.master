    <script>
        $(document).keydown(function (e) {
            try {

                if (e.keyCode > 17 && e.ctrlKey && e.shiftKey) {
                    e.preventDefault();
                    document.getElementById('load').style.visibility = "visible";

                    KeyRedirect('16,17,' + e.keyCode);
                }
                if (e.keyCode > 18 && e.altKey) {
                    document.getElementById('load').style.visibility = "visible";

                    e.preventDefault();
                    KeyRedirect('18,' + e.keyCode);
                }

            } catch (err) {
            }
        });

        function KeyRedirect(keycode) {
            try {
                jQuery.ajax({
                    type: "POST",
                    url: "DashBoard.aspx/GetRedirectUrl", //It calls our web method  
                    contentType: "application/json; charset=utf-8",

                    dataType: "json",
                    data: "{'keycode':'" + keycode + "'}",

                    success: function (msg) {

                        if (msg.d != "") {
                            window.location = msg.d;

                        } else {

                        }
                    },
                    error: function (d) {
                    }
                });
            } catch (err) {

            }

        }

        var req = Sys.WebForms.PageRequestManager.getInstance();
        req.add_beginRequest(function () {
            document.getElementById('load').style.visibility = "visible";
        });
        req.add_endRequest(function () {
            $('.select2').select2();

            document.getElementById('load').style.visibility = "hidden";

        });

        $(document).ready(function () {
            $('.select2').select2();

            // Flexible table
            try {
                $('#example').DataTable();

                // Individual column searching

                // Setup - add a text input to each footer cell
                $('#example6 tfoot th').each(function () {
                    var title = $(this).text();
                    $(this).html('<input class="form-control dataSearch" type="text" placeholder="Search ' + title + '" />');
                });

                // DataTable
                var table = $('#example6').DataTable();

                // Apply the search
                table.columns().every(function () {
                    var that = this;

                    $('input', this.footer()).on('keyup change', function () {
                        if (that.search() !== this.value) {
                            that
                                .search(this.value)
                                .draw();
                        }
                    });
                });

            } catch (err) {
            }

        });
        Sys.WebForms.PageRequestManager.getInstance().add_endRequest(function (evt, args) {
            try {
                $('#example').DataTable();

                // Individual column searching

                // Setup - add a text input to each footer cell
                $('#example6 tfoot th').each(function () {
                    var title = $(this).text();
                    $(this).html('<input class="form-control dataSearch" type="text" placeholder="Search ' + title + '" />');
                });

                // DataTable
                var table = $('#example6').DataTable();

                // Apply the search
                table.columns().every(function () {
                    var that = this;

                    $('input', this.footer()).on('keyup change', function () {
                        if (that.search() !== this.value) {
                            that
                                .search(this.value)
                                .draw();
                        }
                    });
                });
            } catch (err) {
            }
        });

        (function ($) {
